<%= form_for @property, url: property_path, method: :put, html: { class: "your-form-class" } do |f| %>
 
  <%= hidden_field_tag :auth_token, params[:auth_token] %>
  
  Name: <%= f.text_field :name %><br><br>
  District: <%= f.text_field :district %><br><br>
  City: <%= f.text_field :city %><br><br>
  Property Type: <%= f.select :types, Property.types.keys %><br><br>
  MRT Line Station: <%= f.text_field :mrt_line_station %><br><br>
  Beds Number: <%= f.number_field :beds_number %><br><br>
  Rent Per Month: <%= f.number_field :rent_per_month %><br><br>

  <%= f.submit "Submit" %>
<% end %>
